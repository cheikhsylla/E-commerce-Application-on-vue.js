incrementbis(context,payload){\r\n\r\n      setTimeout(function(){\r\n          context.commit('incrementbis',payload)\r\n      },1500)\r\n    },\r\n    Deincrement(context,payload){\r\n\r\n      setTimeout(function(){\r\n          context.commit('Deincrement',payload)\r\n      },1500)\r\n    },\r\n    Deincrementbis(context,payload){\r\n\r\n      setTimeout(function(){\r\n          context.commit('Deincrementbis',payload)\r\n      },1500)\r\n    }\r\n  }\r\n})"]}]}